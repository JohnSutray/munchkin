{
  "name": "action-generator",
  "version": "0.0.1",
  "scripts": {
    "prebuild": "rimraf dist/action-generator && mkdirp ../../dist/action-generator/schematics && copyfiles schematics/**/* ../../dist/action-generator/",
    "build": "tsc -p tsconfig.schematics.json",
    "postbuild": "rimraf --glob ../../dist/action-generator/schematics/**/*.ts"
  },
  "peerDependencies": {
    "@angular/common": "^12.0.0",
    "@angular/core": "^12.0.0"
  },
  "schematics": "./schematics/collection.json"
}
